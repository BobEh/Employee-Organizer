@model WorkerViewModel
@{
    ViewData["Title"] = "Index";
}
<h4>@Model.WorkerTypeName</h4>
<ul class="list-group col-sm-8 col-xs-12">
    @{
        foreach (Worker worker in Model.Workers)
        {
            <li class="list-group-item">
                @worker.LastName, @worker.FirstName
            </li>
        }
    }
</ul>